#tag Events PushButton1
	#tag Event
		Sub Action()
		  
		  dim db as New SQLiteDatabase
		  dim fp as FolderItem = SpecialFolder.Documents.Child("StinkSyncClient")
		  dim PythonModulePath as FolderItem = GetFolderItem( "C:\Users\sean\Desktop\Otis_3.0\StinkSyncClient", FolderItem.PathTypeNative )
		  if not fp.Exists then
		    fp.CreateAsFolder
		  end if
		  
		  dim db_file as string = "MainDb.sqlite"
		  db.DatabaseFile = fp.Child(db_file)
		  if db.CreateDatabaseFile then
		  else
		    MsgBox("Could not connect")
		    Return
		  end if
		  
		  if StinkSync.ServerCheck then
		    Rectangle1.FillColor = RGB(0,255,0)
		  else
		    Rectangle1.FillColor = RGB(255,0,0)
		  end if
		  
		  db.Close
		End Sub
	#tag EndEvent
